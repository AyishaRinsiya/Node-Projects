<html>
    <head>
        <link rel="stylesheet" type="text/css">
        <body>
            <style>
                body {
    font-family: "Roboto", sans-serif;
    background-image: url(im.jpg);
    background-size: cover;
    position: relative;
}
.registration-form{
    position:absolute;
    top:50%;
    left:50%;
    transform:translate(-50%, -50%);
    width:350px;
}
    .registration-form p{
font-size:20px;
margin:15px 0;
    }

.registration-form input {
    font-size:16px;
    padding:15px 10px;
    width:100%;
    border:0;
    border-radius:5px;
    outline:none;
}
.registration-form button {
    font-size:18px;
    font-weight:bold;
    margin:20px 0;
    padding: 10px 15px;
    width:50%;
    border:0;
    border-radius: 5px;
    background-color:#fff;
}
</style>
            <div class="registration-form">
            <form action="mreg" method="post">
                <p>Medicine name:</p>
                   <input type="text" name="medname" placeholder="medname" id="" required >
                <p>Category name:</p>
                <select name="catname">
                    <%
                    for(var i=0;i<data.length;i++){
                        %>
                    <option value="<%=data[i].catid %>"> <%=data[i].catname %> </option> 
                  <%
                    }
                  %>
                    </select>
                  <p>Price:</p>
                  <input type="text" name="price" placeholder="price" id="" required >
                  <p>Qty:</p>
                  <input type="text" name="qty" placeholder="qty" id="" required >
                <p>
                <button type="submit">Register</button>
                </p>
            </form>
        </div>
        </body>
    </head>
</html>